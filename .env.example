# Jina AI API Key (required)
# Get your key at: https://jina.ai/
JINA_AI_KEY=your_jina_api_key_here

# Supabase Configuration (required)
# Get these from your Supabase project settings
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Claude API Key (required)
# Get your key at: https://console.anthropic.com/
CLAUDE_API_KEY=your_claude_api_key_here

# Voyage AI API Key (optional - for vector embeddings)
# Get your key at: https://www.voyageai.com/
# Required for semantic search features
VOYAGE_AI_API_KEY=your_voyage_ai_key_here

# Research Configuration (optional)
MAX_ARTICLES=20
CONCURRENCY=8
BATCH_DELAY_MS=25000
MAX_RETRIES=3
